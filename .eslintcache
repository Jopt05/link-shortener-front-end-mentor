  let x = \"\";\r\n        let links = [...link.data];\r\n        for( let el of link.data ) {\r\n            el.copied = false;\r\n            if( id === el.id ){\r\n                x = link.data.indexOf(el)\r\n                navigator.clipboard.writeText(el.shorten);\r\n            }\r\n        }\r\n        let copiedLink = {...links[x]};\r\n        copiedLink.copied = true;\r\n        links[x] = copiedLink;\r\n        setlink({\r\n            ...link,\r\n            data: links\r\n        })\r\n    }\r\n\r\n    return [link, handleCopy, handleFetch]\r\n}\r\n",{"ruleId":"56","replacedBy":"57"},{"ruleId":"58","replacedBy":"59"},{"ruleId":"60","severity":1,"message":"61","line":25,"column":60,"nodeType":"62","endLine":25,"endColumn":75},{"ruleId":"60","severity":1,"message":"61","line":26,"column":60,"nodeType":"62","endLine":26,"endColumn":75},{"ruleId":"60","severity":1,"message":"61","line":27,"column":60,"nodeType":"62","endLine":27,"endColumn":75},{"ruleId":"60","severity":1,"message":"61","line":28,"column":60,"nodeType":"62","endLine":28,"endColumn":75},{"ruleId":"60","severity":1,"message":"61","line":29,"column":60,"nodeType":"62","endLine":29,"endColumn":75},{"ruleId":"60","severity":1,"message":"61","line":44,"column":60,"nodeType":"62","endLine":44,"endColumn":75},{"ruleId":"60","severity":1,"message":"61","line":56,"column":28,"nodeType":"62","endLine":56,"endColumn":43},{"ruleId":"60","severity":1,"message":"61","line":20,"column":64,"nodeType":"62","endLine":20,"endColumn":79},{"ruleId":"60","severity":1,"message":"61","line":21,"column":64,"nodeType":"62","endLine":21,"endColumn":79},{"ruleId":"60","severity":1,"message":"61","line":22,"column":64,"nodeType":"62","endLine":22,"endColumn":79},{"ruleId":"60","severity":1,"message":"61","line":26,"column":64,"nodeType":"62","endLine":26,"endColumn":79},{"ruleId":"60","severity":1,"message":"61","line":27,"column":64,"nodeType":"62","endLine":27,"endColumn":79},{"ruleId":"60","severity":1,"message":"61"